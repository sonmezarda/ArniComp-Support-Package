// Test 4: Advanced array operations
// Expected: 0 instructions (all compile-time)

byte[5] data;

// Initialize array
data[0] = 1;
data[1] = 2;
data[2] = 3;
data[3] = 4;
data[4] = 5;

// Compute using array elements
byte sum2;
sum2 = data[0] + data[1];  // 1 + 2 = 3

byte sum4;
sum4 = data[2] + data[3];  // 3 + 4 = 7

byte total;
total = sum2 + sum4;  // 3 + 7 = 10

// Store back to array
data[0] = total;  // data[0] = 10
