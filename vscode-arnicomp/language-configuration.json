{
	"comments": { "lineComment": "//" },
	"brackets": [["{", "}"], ["[", "]"], ["(", ")"]],
	"autoClosingPairs": [
		{ "open": "{", "close": "}" },
		{ "open": "[", "close": "]" },
		{ "open": "(", "close": ")" }
	],
	"surroundingPairs": [["{", "}"], ["[", "]"], ["(", ")"]],
	"wordPattern": "[A-Za-z_][A-Za-z0-9_]*"
}
